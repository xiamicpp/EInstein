syntax = "${proto.version}"

option java_package = "${proto.package}"
option java_outer_classname = "${proto.classname}"

message ${proto.message} {

#set($count = 1)
#foreach(${field} in ${proto.fields})
    #if(${field.type} == "double")
    double ${field.name} = ${count};
    #elseif(${field.type} == "float")
    float ${field.name} =  ${count};
    #elseif(${field.type} == "int")
    sint32 ${field.name} =  ${count};
    #elseif(${field.type} == "long")
    sint64 ${field.name} =  ${count};
    #elseif(${field.type} == "boolean")
    bool ${field.name} =  ${count};
    #elseif(${field.type} == "String")
    string ${field.name} =  ${count};
    #elseif(${field.type} == "ByteString")
    bytes ${field.name} =  ${count};
    #end
    #set($count=$count+1)
#end

}
